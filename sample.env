# === Qdrant ===
QDRANT_URL=             # e.g., https://xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx.us-east-1-0.aws.cloud.qdrant.io
QDRANT_API_KEY=         # e.g., qd_xxx... (from Qdrant Cloud)

# === Alpaca News API ===
ALPACA_API_KEY=         # e.g., PKxxxxxxxxxxxxxxxxxxxx
ALPACA_API_SECRET=      # e.g., xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# === Comet ML ===
COMET_API_KEY=          # e.g., 2a1b3c... (from Comet user settings)
COMET_WORKSPACE=        # e.g., your-workspace-name
COMET_PROJECT_NAME=financial-bot

# === Optional AWS (only if using ECR/Docker path) ===
AWS_ECR_REPO_NAME=streaming_pipeline
AWS_REGION=eu-central-1
AWS_PROFILE=default
